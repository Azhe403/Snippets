<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><Wiql>SELECT [System.Id], [System.Title], [System.State] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Task'  AND  [System.AssignedTo] = @me  AND  [System.State] &lt;&gt; 'Closed'  AND  [System.State] &lt;&gt; 'Removed'  AND  [Microsoft.VSTS.CMMI.Blocked] = 'Yes' ORDER BY [System.State] </Wiql></WorkItemQuery>